{
    "manifest_version": 3,
    "name": "AI Prompt Manager",
    "version": "1.0",
    "description": "Store, manage, and quickly use AI prompts.",
    "permissions": [
      "storage",
      "clipboardWrite"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }